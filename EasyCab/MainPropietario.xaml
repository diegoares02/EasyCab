<Window x:Class="EasyCab.MainPropietario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainPropietario" Height="300" Width="346" WindowStartupLocation="CenterScreen">
    <Grid>
        <Menu HorizontalAlignment="Left" Height="19" Margin="10,10,0,0" VerticalAlignment="Top" Width="137" x:Name="mainMenu">
            <MenuItem Header="Personal">
                <MenuItem Header="Conductor" x:Name="menuConductor" Click="menuConductor_Click"></MenuItem>
                <MenuItem Header="Vehiculo" x:Name="menuVehiculo" Click="menuVehiculo_Click"></MenuItem>
                <MenuItem Header="Operador" x:Name="menuOperador" Click="menuOperador_Click" ></MenuItem>
                <MenuItem Header="Dueño" x:Name="menuDueño" Click="menuDueño_Click"></MenuItem>
            </MenuItem>
            <MenuItem Header="Servicio">
                <MenuItem Header="Carreras" x:Name="menuCarreras" Click="menuCarreras_Click"></MenuItem>
                <MenuItem Header="Ubicacion" x:Name="menuUbicacion" Click="menuUbicacion_Click"></MenuItem>
            </MenuItem>
        </Menu>
        <Image HorizontalAlignment="Left" Height="225" Margin="10,34,0,0" VerticalAlignment="Top" Width="318" Source="Imagenes/easy-cab-logo.png" Stretch="Fill"/>
    </Grid>
</Window>
